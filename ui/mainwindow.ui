*/

QMainWindow
{
	background-color: rgb(255, 255, 255)
}

/*
	QListWidget
*/

QListWidget#list__categories
{
	color:#607D8B;
	background-color: white;
	border-top: 0px;
}
QListWidget#list__categories:item
{
	background-color: #ECEFF1;
	margin-bottom:1px;
	padding:5px;
	height: 30px;
	border-radius: 0px
}
QListWidget#list__categories:item:hover
{
	background-color: #f2f2f2;
	border: 0px
}
QListWidget#list__categories:item:selected
{
	background-color: #f2f2f2;
	color:#607D8B;
}

/*************************************/

QListWidget#list__books
{
	border: 1px solid #f2f2f2;
	border-left: 0;
	border-right: 0;
	color: #607D8B;
	background-color: qlineargradient(spread:pad, x1:0.349, y1:0.869, x2:0.283, 		y2:0.744, stop:0 rgba(250, 250, 250, 255), stop:0.330189 rgba(250, 250, 250, 255), stop:0.334906 rgba(250, 250, 250, 255), stop:0.665094 rgba(250, 250, 250, 255), stop:0.67 rgba(244, 244, 244, 255), stop:1 rgba(244, 244, 244, 255))
}

QListWidget#list__books:item
{
	background-color: white;
	border-radius: 2px;
	margin: 8px;
	width: 129px;
	height: 190px
}

QListWidget#list__books:item:hover
{
	background-color: #f2f2f2;
	border: 1px solid #ECEFF1
}

QListWidget#list__books:item:selected
{
	background-color: #f2f2f2;
	border: 1px solid #ECEFF1;
	color: #607D8B
}

