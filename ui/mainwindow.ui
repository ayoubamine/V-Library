{
	width: 200px;
	height: 32px
}

/*
	QToolButton
*/

QToolButton
{
	background: transparent;
	width: 30px;
	padding: 4px;
	border: 0
}

QToolButton:hover
{
	background-color: #ECEFF1;
	border-radius: 3px
}

QToolButton#add__category,
QToolButton#edit__category
{
	border: 1px solid #f2f2f2;
	border-right: 0;
	width: 28px;
	height: 31px;
	padding: 0px;
	border-radius: 0
}

QToolButton#add__category
{
	border-right: 0;
	border-left: 0
}

QToolButton#close:hover
{
	background-color: none
}

/*
